[Service]
WorkingDirectory=@PROJECTDIR@
ExecStart=/usr/bin/node ./node_modules/.bin/msgflo --graph graphs/doorsystem.json --wait-timeout 300 --ignore 2floor --ignore 4floor --ignore frontdoor --ping-interval 60 --broker mqtt://@BROKER@
Restart=always
RestartSec=3
User=msgflo
Environment=PATH=@PROJECTDIR@/node_modules/.bin:/bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin DEBUG=msgflo*

[Unit]
After=network-online.target

[Install]
WantedBy=multi-user.target

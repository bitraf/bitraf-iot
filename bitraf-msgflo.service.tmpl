[Service]
WorkingDirectory=@PROJECTDIR@
ExecStart=/usr/bin/node ./node_modules/.bin/msgflo --graph graphs/doorsystem.json --auto-save=true --broker mqtt://@BROKER@
Restart=always
User=msgflo
Environment=PATH=@PROJECTDIR@/node_modules/.bin:/bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin DEBUG=msgflo*

[Unit]
After=network-online.target

[Install]
WantedBy=multi-user.target
